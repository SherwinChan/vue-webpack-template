{
  "name": "{{name}}",
  "version": "1.0.0",
  "description": "{{description}}",
  "author": "{{author}}",
  "scripts": {
    "dev": "cross-env NODE_ENV=development webpack-dev-server --open --hot --config webpack.config.js",    
    "init": "cross-env NODE_ENV=development webpack --progress --hide-modules --config webpack.dev.config.js",
    "build": "cross-env NODE_ENV=production webpack --progress --hide-modules --config webpack.prod.config.js",
    "lint": "eslint --ext .js,.vue src"
  },
  "element-theme": {
    "browsers": [
      "ie > 9",
      "last 2 versions"
    ],
    "out": "./assets/css/",
    "config": "./element-variables.css",
    "theme": "element-theme-chalk",
    "minimize": false
  },
  "dependencies": {
    "vue": "^2.5.13"{{#router}},
    "vue-router": "^3.0.1"{{/router}}{{#vuex}},
    "vuex": "^3.0.1"{{/vuex}}{{#eruda}},
    "eruda": "^1.3.2"{{/eruda}}
  },
  "devDependencies": {
    "autoprefixer": "^8.6.2",
    "axios": "^0.18.0",
    "babel-core": "^6.26.3",
    "babel-helper-vue-jsx-merge-props": "^2.0.3",
    "babel-loader": "^7.1.1",
    "babel-plugin-component": "^1.1.1",
    "babel-plugin-import": "^1.8.0",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "babel-plugin-syntax-jsx": "^6.18.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-plugin-transform-vue-jsx": "^3.5.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-stage-0": "^6.24.1",
    "babel-preset-stage-1": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "babel-preset-stage-3": "^6.24.1",
    "clean-webpack-plugin": "^0.1.19",
    "copy-webpack-plugin": "^4.3.1",
    "cross-env": "^5.1.3",
    "css-loader": "^0.28.8",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^1.1.6",
    "html-loader": "^0.5.4",
    "html-webpack-plugin": "^2.29.0",
    "less": "^2.7.3",
    "less-loader": "^4.0.4",
    "node-sass": "^4.7.2",
    "postcss": "^6.0.16",

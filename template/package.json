{
  "name": "{{name}}",
  "version": "1.0.0",
  "description": "{{description}}",
  "author": "{{author}}",
  "scripts": {
    "init": "cross-env NODE_ENV=development webpack --progress --config webpack.dev.config.js",
    "dev": "cross-env NODE_ENV=development webpack-dev-server --open --hot --config webpack.dev.config.js",
    "build": "cross-env NODE_ENV=production webpack --progress --hide-modules --config webpack.prod.config.js",
    "lint": "eslint --ext .js,.vue src"
  },
  "element-theme": {
    "browsers": [
      "ie > 9",
      "last 2 versions"
    ],
    "out": "./assets/css/",
    "config": "./element-variables.css",
    "theme": "element-theme-default",
    "minimize": false
  },
  "dependencies": {
    "vue": "^2.4.4"{{#router}},
    "vue-router": "^2.7.0"{{/router}}{{#vuex}},
    "vuex": "^2.4.1"{{/vuex}}
  },
  "devDependencies": {
    "autoprefixer": "^7.1.1",    
    "axios": "^0.16.2",
    "babel-core": "^6.25.0",
    "babel-helper-vue-jsx-merge-props": "^2.0.2",
    "babel-loader": "^7.1.1",
    "babel-plugin-component": "^0.9.1",
    "babel-plugin-syntax-jsx": "^6.18.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",    
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-plugin-transform-vue-jsx": "^3.5.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-stage-0": "^6.24.1",
    "babel-preset-stage-1": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "babel-preset-stage-3": "^6.24.1",
    "copy-webpack-plugin": "^4.0.1",
    "cross-env": "^5.0.1",
    "css-loader": "^0.28.4",
    "extract-text-webpack-plugin": "^3.0.1",
    "file-loader": "^0.11.2",
    "html-loader": "^0.4.5",
    "html-webpack-plugin": "^2.29.0",
    "less": "^2.7.2",
    "less-loader": "^4.0.4",
    "node-sass": "^4.5.3",
    "postcss": "^6.0.3",
    "postcss-cssnext": "^2.11.0",
    "postcss-loader": "^2.0.6",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.18.2",
    "url-loader": "^0.5.9",{{#vconsole}}
